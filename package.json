{
  "name": "webscrapping",
  "version": "0.0.1",
  "description": "Sistema RPA para scraping PJUD Chile - Compatible con Node 16.20.2+",
  "main": "index.js",
  "engines": {
    "node": ">=16.20.2 <18.0.0"
  },
  "scripts": {
    "test": "node src/test/run-tests.js",
    "test:unit": "node src/test/run-tests.js",
    "test:5causas": "node src/test/scraper-5-causas.js",
    "test:5causas:dry": "node src/test/scraper-5-causas.js --dry-run",
    "scrape": "node src/index.js",
    "scrape:batch": "node src/process-causas.js",
    "scrape:masivo": "node src/scraping_masivo.js",
    "scrape:test": "node src/test/scraper-5-causas.js",
    "backup:clean": "node src/backup_and_clean.js",
    "db:setup": "mysql < database/schema_mariadb_5.5.sql",
    "db:test": "node src/database/db-mariadb.js",
    "importar:movimientos": "node src/importar_movimientos_sql.js",
    "api:start": "node src/api/server.js",
    "api:listener": "node src/api/listener.js",
    "api:listener:causa": "node src/api/listener.js --table causa",
    "api:listener:agenda": "node src/api/listener.js --table agenda",
    "test:carga": "node scripts/test-carga.js",
    "test:carga:completo": "node scripts/test-carga.js --scenarios 4 --duration 30",
    "monitor": "bash scripts/monitor-recursos.sh",
    "services:start": "bash scripts/control-servicios.sh start",
    "services:stop": "bash scripts/control-servicios.sh stop",
    "services:restart": "bash scripts/control-servicios.sh restart",
    "services:status": "bash scripts/control-servicios.sh status",
    "services:enable": "bash scripts/control-servicios.sh enable",
    "services:disable": "bash scripts/control-servicios.sh disable",
    "postinstall": "npx playwright install firefox"
  },
  "keywords": [
    "scraping",
    "rpa",
    "pjud",
    "chile",
    "playwright"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "playwright": "1.39.0",
    "playwright-core": "^1.39.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mysql2": "^3.6.5",
    "nodemailer": "^7.0.13",
    "winston": "^3.11.0"
  }
}
